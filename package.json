{
	"scripts": {
		"convert": "ts-node convert-csv.ts",
		"format": "prettier --write .",
		"test": "export NODE_ENV=development && playwright test -c ./tests/playwright.config.ts",
		"test:debug": "export NODE_ENV=development && playwright test --debug -c ./tests/playwright.config.ts",
		"test:trace": "export NODE_ENV=development && playwright test --trace on -c ./tests/playwright.config.ts",
		"test:report": "playwright show-report",
		"run": "pnpm test:debug && pnpm convert"
	},
	"dependencies": {
		"@playwright/test": "^1.50.1",
		"dotenv": "^16.4.7"
	},
	"devDependencies": {
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"@types/node": "^22.13.0",
		"prettier": "^3.4.2",
		"ts-node": "^10.9.2"
	},
	"packageManager": "pnpm@9.9.0+sha512.60c18acd138bff695d339be6ad13f7e936eea6745660d4cc4a776d5247c540d0edee1a563695c183a66eb917ef88f2b4feb1fc25f32a7adcadc7aaf3438e99c1"
}
